    scanf("%f", &principal);

    printf("Enter the rate of interest: ");
    scanf("%f", &rate);

    printf("Enter the time (in years): ");
    scanf("%d", &time);
    float simpleInterest = calculateSimpleInterest(principal, rate, time);
    printf("Simple Interest: %.2f\n", simpleInterest);

    return 0;
}


16. dec to binary

#include <stdio.h>

int main() {
    int decimalNumber;
    printf("Enter a decimal number: ");
    scanf("%d", &decimalNumber);
    printf("Binary representation: ");
    int size = sizeof(decimalNumber) * 8;

    for (int i = size - 1; i >= 0; i--) {
        int bit = (decimalNumber >> i) & 1;
        printf("%d", bit);
    }

    return 0;
}

17.gcd and lcm 

#include <stdio.h>

int main() {
    int num1, num2;

   
    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    
    int a = num1, b = num2;
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    int gcd = a;

    
    int lcm = (num1 * num2) / gcd;

    
    printf("GCD: %d\n", gcd);
    printf("LCM: %d\n", lcm);

    return 0;
}
18.sample interest

#include <stdio.h>

int main() {
    float principal, rate, time, simpleInterest;

   
    printf("Enter principal amount: ");
    scanf("%f", &principal);

    printf("Enter rate of interest: ");
    scanf("%f", &rate);

    printf("Enter time (in years): ");
    scanf("%f", &time);

    
    simpleInterest = (principal * rate * time) / 100;

    
    printf("Simple Interest: %.2f\n", simpleInterest);

    return 0;
}

19.sorted string

#include <stdio.h>
#include <string.h>

#define MAX_LENGTH 100
#define MAX_STRINGS 50

int main() {
    int n;

    printf("Enter the number of strings: ");
    scanf("%d", &n);

    char strings[MAX_STRINGS][MAX_LENGTH];

    for (int i = 0; i < n; i++) {
        printf("Enter string %d: ", i + 1);
        scanf("%s", strings[i]);
    }

    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (strcmp(strings[j], strings[j + 1]) > 0) {
                char temp[MAX_LENGTH];
                strcpy(temp, strings[j]);
                strcpy(strings[j], strings[j + 1]);
                strcpy(strings[j + 1], temp);
            }
        }
    }

    printf("\nSorted strings:\n");
    for (int i = 0; i < n; i++) {
        printf("%s\n", strings[i]);
    }

    return 0;
}

20.reversed string

#include <stdio.h>
#include <string.h>
int main() {
    char inputString[MAX_LENGTH];
    printf("Enter a string: ");
    scanf("%s", inputString);
    int length = strlen(inputString);
    for (int i = 0, j = length - 1; i < j; i++, j--) {
     
        char temp = inputString[i];
        inputString[i] = inputString[j];
        inputString[j] = temp;
    }

    
    printf("Reversed string: %s\n", inputString);

    return 0;
}
21.mean,median and mode

#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    float sum = 0, mean, median;
    int modeValue = -1, maxCount = 0, count = 1;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    int arr = (int)malloc(n * sizeof(int));

    printf("Enter %d elements: ", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    mean = sum / n;

    qsort(arr, n, sizeof(int), compare);

    if (n % 2 == 0)
        median = (arr[n / 2 - 1] + arr[n / 2]) / 2.0;
    else
        median = arr[n / 2];

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j])
                count++;
        }
        if (count > maxCount) {
            maxCount = count;
            modeValue = arr[i];
        }
        count = 1;
    }

    printf("Mean: %.2f\n", mean);
    printf("Median: %.2f\n", median);
    printf("Mode: %d\n", modeValue);

    free(arr);
    return 0;
}
22.duplicate

#include <stdio.h>
#define MAX_SIZE 50
int main() {
    int size;

    printf("Enter the size of the array (max %d): ", MAX_SIZE);
    scanf("%d", &size);

    if (size <= 0 || size > MAX_SIZE) {
        printf("Invalid array size.\n");
        return 1;
    }

    int arr[MAX_SIZE];

    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }

    printf("\nDuplicate elements in the array:\n");
    for (int i = 0; i < size - 1; i++) {
        if (arr[i] != -1) {
            for (int j = i + 1; j < size; j++) {
                if (arr[i] == arr[j]) {
                    printf("%d\n", arr[i]);
                    arr[j] = -1;
                }
            }
        }
    }

    return 0;
}

23.max and min

#include <stdio.h>

int main() {
    int n; 
    printf("Enter the size of the array: ");
    scanf("%d", &n);

    int arr[n]; // Declare an array of size n

    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int max = arr[0];
    int min = arr[0];

    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
        if (arr[i] < min) {
            min = arr[i];
        }
    }

    printf("Maximum: %d\n", max);
    printf("Minimum: %d\n", min);

    return 0;
}
