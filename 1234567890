{ 
 	int i,j,rows,columns;  	printf("enter number of rows:"); 
 	scanf("%d",&rows); 
 	printf("enter number of columns:"); 
 	scanf("%d",&columns);  	for(i=1;i<=rows;i++) 
 	{ 
 	 	for(j=1;j<=columns;j++) 
 	 	{ 
 	 	 	printf("#"); 
 	 	} 
 	 	printf("\n"); 
 	} 
 	return 0; 
