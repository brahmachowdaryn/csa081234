#include<stdio.h> int main(){ 
 	int r,i,j,n=1;  	printf("Enter no.of rows: ");  	scanf("%d",&r);  	for(i=1;i<=r;i++){ 
 	 	for(j=1;j<=i;++j){  	 	 	printf("%d\t",n); 
 	 	 	++n; 
 	 	} 
 	 	printf("\n\n"); 
 	} 
 	return 0; 
}
 
PASCAL TRIANGLE 
#include<stdio.h> void printPascal(int n){ 
 	for(int j=1;j<=n;j++){ 
 	 	int B=1; 
 	 	for(int i=1;i<=j;i++){ 
 	 	 	printf("%d",B);  	 	 	B=B*(j-i)/i; 
 	 	} 
 	 	printf("\n"); 
 	} 
} 
int main(){ 
 	int rows; 
 printf("Enter no.of rows: ");  scanf("%d",&rows);  printPascal(rows);  return 0; 
} 
