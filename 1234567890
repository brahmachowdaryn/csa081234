RHOMBUS PATTERN 
#include<stdio.h> int main(){ 
 	int i,j,n=5;  	char ch='*'; 
 	for(i=1;i<=n;i=i+2){ 
 	 	for(j=1;j<=i;j++){ 
 	 	 	printf("%c",ch); 
 	 	} 
 	 	printf("\n"); 
 	} 
 	for(i=n-2;i>=1;i=i-2){ 
 	 	for(j=1;j<=i;j++){ 
 	 	 	printf("%c",ch); 
 	 	} 
 	 	printf("\n"); 
 	} 
             Return 0; 
} 
OUTPUT:- 
* 
*** 
***** 
*** 
* 
 
