    { 
     	if((s[i]>=65 && s[i]<=90)|| (s[i]>=97 && s[i]<=122)) 
     	{ 
 	 
if(s[i]=='a'|| 
s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O' ||s[i]=='U') 
vowels++; 
            else              consonants++; 
        } 
   	} 
  	printf("vowels = %d\n",vowels);     printf("consonants = %d\n",consonants);     printf("Repeated characters:");     for (i = 0; i < len; i++)  
    { 
        for (j = i + 1; j < len; j++)  
        { 
            if (s[i] == s[j]) 
            { 
                if (v == 1 && s[j] != '\0') 
                { 
                    printf("%c", s[i]); 
                }                                  s[j] = '\0';                 v++;             }           }         v = 1; 
        }         return 0; } 
