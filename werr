EVEN NUMBER AND ODD NUMBER 
#include<stdio.h> int main(){ 
 	int n; 
 	printf("Enter the number:\n");  	scanf("%d",&n);  	if(n%2==0){  	 	printf("%d is an Even number\n",n); 
 	}else{ 
 	 	printf("%d is an Odd number\n",n); 
 	} 
 	return 0; 
} 
SUM OF EVEN AND ODD 
#include<stdio.h> int main(){ 
 int n,I,even_sum=0,odd_sum=0;  printf("enter the value:");  scanf("%d",&n);  for(i=1;i<=n;i++){ 
 	 	if(i%2==0){  	 	  even_sum+=i; 
 	 	}else{ 
 	 	  odd_sum+=i; 
 	 	} 
 	} 
 	printf("Sum of even numbers:%d\n",even_sum);  	printf("Sum of odd numbers:%d\n",odd_sum); 
 	return 0; 
} 
