 	char name[10];  	int rollno;  	float marks; 
}s; int main() 
{ 
 	printf("enter the name:");  	scanf("%s",s.name);  	printf("enter the rollno:");  	scanf("%d",s.rollno);  	printf("enter the marks:");  	scanf("%f",s.marks);  	printf("name:%s\n",s.name);  	printf("roll:%d\n",s.rollno);  	printf("marks:%f\n",s.marks); 
 	return 0; 
} 
